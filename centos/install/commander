sudo wget https://github.com/jhasselbring/commander-endpoint/releases/download/v0.1.0/Commander-linux -o /opt/ninja/commander && sudo chmod +x /opt/ninja/commander && sudo wget https://raw.githubusercontent.com/jhasselbring/s/master/centos/install/commander-files/commander.service -o /lib/systemd/system/commander.service && sudo systemctl daemon-reload 